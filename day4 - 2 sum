class Solution {
    public int[] twoSum(int[] nums, int target) {
        int num = 0;
        int arr[] = new int[2];
        for(int i=0;i<nums.length;i++){
            num = target - nums[i];
            for(int j=i+1;j<nums.length;j++){
                if(num == nums[j]){
                    arr[0] = i;
                    arr[1] = j;
                    break;
                }
            }
        }
        return arr;
    }
}
